{
  "rewrites": [
    {
      "source": "/qr/:path*",
      "destination": "/apps/qr/:path*"
    },
    {
      "source": "/qr",
      "destination": "/apps/qr/index.html"
    },
    {
      "source": "/unfollow/:path*",
      "destination": "/apps/unfollow/:path*"
    },
    {
      "source": "/unfollow",
      "destination": "/apps/unfollow/index.html"
    },
    {
      "source": "/drinkmaster/:path*",
      "destination": "/apps/drinkmaster/:path*"
    },
    {
      "source": "/drinkmaster",
      "destination": "/apps/drinkmaster/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    },
    {
      "source": "/(.*)\\.(js|css|svg|png|jpg|jpeg|gif|ico|woff|woff2)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
