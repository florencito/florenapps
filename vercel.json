{
  "functions": {
    "apps/qr/index.html": {
      "runtime": "@vercel/static"
    },
    "apps/unfollow/index.html": {
      "runtime": "@vercel/static"
    },
    "apps/drinkmaster/index.html": {
      "runtime": "@vercel/static"
    }
  },
  "routes": [
    {
      "src": "/qr/(.*)",
      "dest": "/apps/qr/$1"
    },
    {
      "src": "/unfollow/(.*)",
      "dest": "/apps/unfollow/$1"
    },
    {
      "src": "/drinkmaster/(.*)",
      "dest": "/apps/drinkmaster/$1"
    },
    {
      "src": "/qr/?$",
      "dest": "/apps/qr/index.html"
    },
    {
      "src": "/unfollow/?$",
      "dest": "/apps/unfollow/index.html"
    },
    {
      "src": "/drinkmaster/?$",
      "dest": "/apps/drinkmaster/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    },
    {
      "source": "/(.*)\\.(js|css|svg|png|jpg|jpeg|gif|ico|woff|woff2)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
